{
  "name": "genijalno-maps-client-react",
  "version": "1.0.0",
  "description": "React redux google maps interaction",
  "main": "index.js",
  "scripts": {
    "start": "npm run devHot",
    "devHot": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --port 1234",
    "watch": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --progress --colors --watch --port 1234",
    "build": "npm run clean && node ./node_modules/webpack/bin/webpack.js",
    "prod": "./node_modules/webpack/bin/webpack.js -p --config webpack.prod.config.js",
    "postinstall": "npm run build",
    "clean": "rm -rf dist"
  },
  "author": "Benjamin Babic",
  "license": "ISC",
  "browsersList": [
    "last 3 versions",
    "ie 10"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "svg-react-loader": "^0.4.5"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "react-test-renderer": "^15.6.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(css)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/src/test/setup.js"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
